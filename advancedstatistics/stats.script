CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE CACHED TABLE TORRENT(ID BIGINT NOT NULL PRIMARY KEY,NAME VARCHAR(255) NOT NULL)
CREATE CACHED TABLE TORRENT_COMPLETED_DATA(ID BIGINT NOT NULL,COMPLETED BIGINT NOT NULL,TIME_COMPLETED BIGINT NOT NULL,PRIMARY KEY(ID,COMPLETED))
CREATE CACHED TABLE TORRENT_TRANSFER_DATA(ID BIGINT NOT NULL,TRANSFER_DATE BIGINT NOT NULL,COMPLETED_TO_DATE BIGINT NOT NULL,RECEIVED_TO_DATE BIGINT NOT NULL,DISCARDED_TO_DATE BIGINT NOT NULL,SENT_TO_DATE BIGINT NOT NULL,RECEIVED BIGINT NOT NULL,DISCARDED BIGINT NOT NULL,SENT BIGINT NOT NULL,UP_TIME BIGINT NOT NULL,PRIMARY KEY(ID,TRANSFER_DATE))
CREATE CACHED TABLE TRANSFER_DATA(TRANSFER_DATE BIGINT NOT NULL PRIMARY KEY,RECEIVED BIGINT NOT NULL,DISCARDED BIGINT NOT NULL,SENT BIGINT NOT NULL,PROT_RECEIVED BIGINT NOT NULL,PROT_SENT BIGINT NOT NULL,UP_TIME BIGINT NOT NULL)
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
